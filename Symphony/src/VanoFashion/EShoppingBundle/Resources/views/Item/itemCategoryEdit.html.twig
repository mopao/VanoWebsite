
{% extends "VanoFashionEShoppingBundle:Item:websiteManagement.html.twig" %}


{% block title %}

   {{ parent() }} - add items category

{% endblock %}


{% block vanofashioneshopping_management_body %}


    
	<div class="page-header">
	<h1 class="text-center"> Modified  a category </h1>
	</div>

	{% for label, messages in app.flashes %}
	    {% for message in messages %}
	        <div class="alert alert-{{ label }}">
	           <STRONG> {{label}}: </STRONG> {{ message }}
	        </div>
	    {% endfor %}
    {% endfor %}
	<div >

    <h3> Complete the form below to modify the category   </h3>
    {{ include("VanoFashionEShoppingBundle:Item:itemCategoryForm.html.twig") }}

    </div>

{% endblock %}